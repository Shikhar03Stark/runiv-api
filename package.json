{
  "name": "runiv-api",
  "version": "1.0.0",
  "description": "REST API for Runiv",
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --recursive --exit",
    "start": "npm run migrate:all; NODE_ENV=production node server.js",
    "dev": "npm run migrate:all; npm run seed:all; npx nodemon server.js",
    "initdb": "npx sequelize-cli db:create",
    "migrate:all": "npx sequelize-cli db:migrate",
    "seed:all": "npx sequelize-cli db:seed:all",
    "undo:seed": "npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [
    "rest",
    "express"
  ],
  "author": "Harshit Vishwakarma",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "faker": "^5.5.3",
    "mocha": "^9.0.2",
    "nodemon": "^2.0.9",
    "sequelize-cli": "^6.2.0"
  }
}
